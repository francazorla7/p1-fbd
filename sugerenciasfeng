radar{
	carretera
	sentido
	velocidad maxima
	punto kilometrico 
	observacion
}

observacion{
	fecha
	hora
	matricula vehiculo
	sancion
	velocidad 
	lapse de tiempo 
}

sancion{
	dni_propietario
	fecha de envio
	cuantia
	forma de pago
	estado
	penalizacion (true or false)
}

alegacion{
	sancion(enlaza a las claves primarias de sancion)
	dni_propieratio
	fecha_registrada
	estado
	fecha_ejecucion
	
}
